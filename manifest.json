{
    "manifest_version": 3,
    "name": "on-page Jisho",
    "version": "0.0.1",
    "description": "jisho definitions and kanji strokes on page",
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "templates/templates.js",
                "scripts/content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    }
}